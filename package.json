{
  "name": "pushsafer-notifier",
  "display_name": "Pushsafer",
  "version": "0.1.2",
  "description": "Send a notification to your pushsafer app (gateway >= 0.9.0 only)",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tim-hellhake/pushsafer-notifier.git"
  },
  "author": "Tim Hellhake",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/tim-hellhake/pushsafer-notifier/issues"
  },
  "homepage": "https://github.com/tim-hellhake/pushsafer-notifier#readme",
  "files": [
    "LICENSE",
    "SHA256SUMS",
    "lib/index.js",
    "lib/pushsafer-notifier.js",
    "manifest.json"
  ],
  "moziot": {
    "api": {
      "min": 2,
      "max": 2
    },
    "plugin": true,
    "exec": "{nodeLoader} {path}",
    "type": "notifier",
    "schema": {
      "type": "object",
      "required": [
        "apiKey"
      ],
      "properties": {
        "apiKey": {
          "type": "string",
          "title": "The api key"
        }
      }
    }
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.2",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "typescript": "^3.6.4",
    "@types/node": "^12.11.0"
  }
}
